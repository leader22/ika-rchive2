    "babel-loader": "^7.0.0",
    "babel-preset-react": "^6.24.1",
    "flow-bin": "^0.47.0",
    "node-sass": "^4.5.3",
    "uglify-es-webpack-plugin": "0.0.2",
    "webpack": "^3.0.0"
  },
  "dependencies": {
    "mobx": "^3.1.11",
    "mobx-react": "^4.2.1",
    "react": "^15.6.0",
    "react-dom": "^15.6.0",
    "react-highcharts": "^12.0.0"
  }
}
